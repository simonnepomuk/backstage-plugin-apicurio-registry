{
  "include": ["./src/**/*"],
  "exclude": ["./dist", "./node_modules"],
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noUncheckedIndexedAccess": true,
    "baseUrl": ".",
    "paths": {
      "@generated/*": ["./generated/*"],
    },
    "jsx": "react"
  }
}